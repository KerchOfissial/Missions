dict = {"Ловкость":0,"Сила":0,"Скорость":0,"Интелект":0,"Скалолазание":0}

print(dict)

ochk = 5

while ochk != 0:
    inp = input("Введите характеристику(Ловкость,Сила,Скорость,Интелект,Скалолазание: ")
    if inp not in dict.keys():
        print("Такого значения нет. Попробуйте ещё раз")
    else:
        ochk -= 1
        dict[inp] += 1

    print(dict)
