import  random

dict = {'Привет':["Привет","Ку","Дароу","Приветствую","Честь имею"],"Как дела":["Норм","Каеф","Жить можно","Не всё так плохо"], "Ты хороший":["Спасибо, ты тоже","Спасибо"]}

while True:
    inp = input("Введите сообщение: ")

    if inp in dict:
        print(random.choice(dict[inp]))
        continue

    if inp not in dict and inp != "Пока":
        print("Бот не знает такое:(")
        continue
    if inp == "Пока":
        break
