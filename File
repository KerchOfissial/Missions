import random

spisok = []
sp2 = []
n = int(input("Введите количество чисел: "))

for i in range(n):
    spisok.append(random.randint(1,4))

print(spisok)

def count_nums(spisok,target):
    return [target] * spisok.count(target)
    
for i in range(1,5):
    sp_tmp = count_nums(n, i)
    sp2.append(count_nums(n,i))


print(sp2)
