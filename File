import random

first_sp = []
sp2 = []
n = int(input("Введите количество чисел: "))

for i in range(n):
    first_sp.append(random.randint(1,4))

print(first_sp)

def count_nums(sp1,target):
    return [target] * sp1.count(target)
    
for i in range(1,5):
    sp_tmp = count_nums(n, i)
    sp2.append(count_nums(n,i))


print(sp2)
